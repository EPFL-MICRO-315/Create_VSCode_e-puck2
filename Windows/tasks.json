{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "DFU EPuck-2-Main_Processor (EPuck Monitor)",
            "type": "shell",
            "command": "${config:epuck2_utils}//dfu-util.exe -d 0483:df11 -a 0 -s 0x08000000 -D ${config:epuck2_utils}//e-puck2_main-processor.bin",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "Link Librairy to workspace",
            "type": "shell",
            "command": "C:/Windows/System32/cmd.exe /c \"mklink /J e-puck2_main-processor ${config:workplaceAS}\\Lib\\e-puck2_main-processor\"",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}